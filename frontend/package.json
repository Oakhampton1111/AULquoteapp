{
  "name": "@aul-quote-app/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "jest --config=jest.config.cjs",
    "test:unit": "jest --config=jest.config.cjs --testPathPattern=src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "jest --config=jest.config.cjs --coverage",
    "test:prod": "cross-env SKIP_VISUAL_TESTS=true jest --config=jest.config.cjs \"QuoteForm.standalone.test.tsx|QuoteForm.ux.standalone.test.tsx\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "deploy": "node deploy.js",
    "deploy:prod": "cross-env NODE_ENV=production node deploy.js",
    "analyze": "vite build --mode production --analyze"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "4.36.1",
    "antd": "^5.0.0",
    "axios": "^1.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "use-sync-external-store": "1.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.0.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.9.2",
    "html-to-image": "^1.11.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-image-snapshot": "^6.4.0",
    "msw": "^2.7.5",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "styled-components": "5.3.11",
    "terser": "^5.39.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
