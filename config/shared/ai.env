# Shared AI/LLM Configuration

# Model Configuration
LLM_MODEL_NAME=flan-t5-base
LLM_TEMPERATURE=0.1
LLM_MAX_LENGTH=512
LLM_TOP_P=0.9
LLM_TOP_K=50

# RAG Configuration
RAG_INDEX_NAME=aul-quote-knowledge
RAG_CHUNK_SIZE=512
RAG_CHUNK_OVERLAP=50
EMBEDDING_MODEL=all-MiniLM-L6-v2
